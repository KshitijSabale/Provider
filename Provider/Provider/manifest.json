{
    "manifest_version": 3,
    "name": "Provider",
    "version": "1.2",

    "description": "Prevents You From wasting time on youtube ",
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },

    "action": {
    "default_title": "Provider",
    "default_popup": "popup.html"
    },
    "permissions": [
      "storage",
      "tabs",
      "scripting",
      "webNavigation"
      ],
      "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline'  https://maxcdn.bootstrapcdn.com;"
      },
    "background": {
      "service_worker": "background-wrapper.js"
    },
    "host_permissions":["https://www.youtube.com/watch?*"]
}
